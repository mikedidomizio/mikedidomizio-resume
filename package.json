{
  "name": "mikedidomizio-resume",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "ci:test": "NODE_CI=true yarn test",
    "test": "yarn test:unit && yarn test:snapshot && yarn test:integration && yarn test:visual && yarn test:pdf",
    "test:unit": "./node_modules/.bin/jest --coverage -- ./src/__tests__/App.spec.js ./src/components/.*.spec.js",
    "test:snapshot": "./node_modules/.bin/jest -- ./src/__tests__/App.snapshot.spec.js",
    "test:integration": "./node_modules/.bin/jest -- src/__tests__/App.integration.spec.js",
    "test:visual": "./node_modules/.bin/jest -- src/__tests__/App.screenshot.spec.js",
    "test:pdf": "./node_modules/.bin/jest -- src/__tests__/App.pdf.spec.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.3.0",
    "jest-serializer-vue": "^2.0.2",
    "pdfreader": "^1.2.14",
    "puppeteer": "16",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/core": "^7.12.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {"vue/multi-word-component-names": "off"}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testURL": "http://localhost/",
    "globals": {
      "vue-jest": {
        "babelConfig": true
      }
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js"
    ],
    "coveragePathIgnorePatterns": [
      ".*__snapshots__/.*"
    ]
  },
  "resolutions": {
    "@achrinza/node-ipc": "10.1.9"
  }
}
